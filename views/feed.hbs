<div class="feed-items">
    {{#each items}}
        <article data-uuid="{{id}}">
            <img src="{{ image.url }}">
            <h2><img class="avatar" src="{{ subject.avatar.url }}"> {{ subject.name }}</h2>
            <h1><a href="appearance/{{id}}">{{title}}</a></h1>
        </article>
    {{/each}}
</div>